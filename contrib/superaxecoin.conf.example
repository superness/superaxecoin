# SuperAxeCoin Core configuration file
# See doc/superaxecoin.conf.md for options

# Network
server=1
listen=1

# RPC settings
rpcuser=superaxecoinrpc
rpcpassword=CHANGE_THIS_PASSWORD
rpcport=8832
rpcallowip=127.0.0.1

# Mining (for Bitaxe compatibility)
# rpcworkqueue=64

# Performance
dbcache=450
par=0
maxmempool=300

# Network settings
# For mainnet (default)
# port=8833

# For testnet, uncomment:
# testnet=1
# [test]
# port=18833
# rpcport=18832

# For regtest, uncomment:
# regtest=1
# [regtest]
# port=18444
# rpcport=18443
